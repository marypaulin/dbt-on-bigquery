Table distribution_centers {
  id int64 [pk]
  name string
  distribution_center_geom geography
}

Table events {
  id int64 [pk]
  user_id int64 [ref: > users.id]
  session_id string
  created_at timestamp
  ip_address string
  browser string
}

Table inventory_items {
  id int64 [pk]
  product_id int64 [ref: > products.id]
  cost float64
  product_category string
  product_name string
  product_distribution_center_id int64 [ref: > distribution_centers.id]
}

Table order_items {
  id int64 [pk]
  order_id int64 [ref: > orders.order_id]
  user_id int64 [ref: > users.id]
  product_id int64 [ref: > products.id]
  inventory_item_id int64 [ref: > inventory_items.id]
  status string
  created_at timestamp
  shipped_at timestamp
}

Table orders {
  order_id int64 [pk]
  user_id int64 [ref: > users.id]
  status string
  created_at timestamp
  shipped_at timestamp
  num_of_item int64
}

Table products {
  id int64 [pk]
  cost float64
  category string
  name string
  distribution_center_id int64 [ref: > distribution_centers.id]
}

Table users {
  id int64 [pk]
  first_name string
  last_name string
  email string
  age int64
  gender string
  state string
}
